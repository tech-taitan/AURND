# Database
DATABASE_URL="postgresql://postgres:your_password_here@localhost:5433/rd_tax_app?schema=public"

# NextAuth.js
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"

# Encryption (required for TFN and sensitive data)
# Generate with: openssl rand -base64 32
ENCRYPTION_KEY="your-encryption-key-here-generate-with-openssl-rand-base64-32"

# Logging (optional)
LOG_LEVEL="info"

# Node environment
NODE_ENV="development"

# Optional: OAuth Providers
# GOOGLE_CLIENT_ID=""
# GOOGLE_CLIENT_SECRET=""

# Optional: Email Provider (for magic links)
# EMAIL_SERVER_HOST=""
# EMAIL_SERVER_PORT=""
# EMAIL_SERVER_USER=""
# EMAIL_SERVER_PASSWORD=""
# EMAIL_FROM=""

# Optional: File Storage (AWS S3 or Cloudflare R2)
# AWS_ACCESS_KEY_ID=""
# AWS_SECRET_ACCESS_KEY=""
# AWS_REGION=""
# AWS_S3_BUCKET=""

# Optional: OCR Python path
# OCR_PYTHON_PATH=""

# Optional: Google AI (Gemini) for AI-powered features
# Get your API key from: https://aistudio.google.com/app/apikey
# GOOGLE_AI_API_KEY=""

# Optional: Australian Business Register GUID (for ABN lookup)
# Get your GUID from: https://abr.business.gov.au/Tools/WebServices
# ABR_GUID=""
