{
  "name": "rd-tax-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./node_modules/next/dist/bin/next dev",
    "build": "node ./node_modules/prisma/build/index.js generate && node ./node_modules/next/dist/bin/next build",
    "start": "node ./node_modules/next/dist/bin/next start",
    "lint": "node ./node_modules/eslint/bin/eslint.js .",
    "test": "node ./node_modules/vitest/vitest.mjs run",
    "test:watch": "node ./node_modules/vitest/vitest.mjs",
    "test:coverage": "node ./node_modules/vitest/vitest.mjs run --coverage",
    "test:e2e": "npx playwright test",
    "seed:admin": "node ./node_modules/tsx/dist/cli.cjs src/scripts/seed-admin.ts",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d",
    "db:push": "node ./node_modules/prisma/build/index.js db push",
    "db:studio": "node ./node_modules/prisma/build/index.js studio",
    "db:seed": "node ./node_modules/prisma/build/index.js db seed",
    "db:generate": "node ./node_modules/prisma/build/index.js generate"
  },
  "prisma": {
    "seed": "node ./node_modules/tsx/dist/cli.cjs prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
